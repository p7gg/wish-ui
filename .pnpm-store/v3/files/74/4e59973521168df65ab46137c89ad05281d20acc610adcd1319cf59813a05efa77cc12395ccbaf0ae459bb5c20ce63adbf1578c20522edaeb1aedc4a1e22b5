import { CalendarDate } from "@internationalized/date";
import { Accessor } from "solid-js";
import { CalendarState, RangeCalendarState } from "./types";
export declare function createSelectedDateDescription(state: CalendarState | RangeCalendarState): Accessor<string>;
interface CreateVisibleRangeDescriptionProps {
    startDate: Accessor<CalendarDate>;
    endDate: Accessor<CalendarDate>;
    timeZone: Accessor<string>;
    isAria: Accessor<boolean>;
}
export declare function createVisibleRangeDescription(props: CreateVisibleRangeDescriptionProps): Accessor<string>;
export {};
