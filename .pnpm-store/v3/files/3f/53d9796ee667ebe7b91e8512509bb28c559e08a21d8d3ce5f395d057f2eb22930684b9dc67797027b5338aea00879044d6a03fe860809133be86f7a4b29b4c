import { CalendarDate } from "@internationalized/date";
import { Accessor } from "solid-js";
export interface CalendarCellContextValue {
    isSelectable: Accessor<boolean>;
    isDisabled: Accessor<boolean>;
    isUnavailable: Accessor<boolean>;
    isSelected: Accessor<boolean>;
    isInvalid: Accessor<boolean>;
    date: Accessor<CalendarDate>;
    label: Accessor<string>;
    nativeDate: Accessor<Date>;
}
export declare const CalendarCellContext: import("solid-js").Context<CalendarCellContextValue>;
export declare function useCalendarCellContext(): CalendarCellContextValue;
