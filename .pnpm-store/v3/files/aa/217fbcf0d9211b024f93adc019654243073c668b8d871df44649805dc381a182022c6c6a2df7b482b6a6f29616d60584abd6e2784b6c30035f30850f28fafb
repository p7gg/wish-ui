import { OverrideProps } from "@kobalte/utils";
import { ComponentProps } from "solid-js";
export interface RadioGroupItemOptions {
    /**
     * The value of the radio button, used when submitting an HTML form.
     * See [MDN](https://developer.mozilla.org/en-US/docs/Web/HTML/Element/input/radio#Value).
     */
    value: string;
    /** Whether the radio button is disabled or not. */
    isDisabled?: boolean;
}
/**
 * The root container for a radio button.
 */
export declare function RadioGroupItem(props: OverrideProps<ComponentProps<"label">, RadioGroupItemOptions>): import("solid-js").JSX.Element;
