import { MenuItemBaseOptions } from "./menu-item-base";
export interface MenuRadioItemOptions extends Omit<MenuItemBaseOptions, "isChecked" | "isIndeterminate"> {
    /** The value of the menu item radio. */
    value: string;
}
/**
 * An item that can be controlled and rendered like a radio.
 */
export declare const MenuRadioItem: import("@kobalte/utils").PolymorphicComponent<"div", MenuRadioItemOptions>;
