        value: context,
        get children() {
          return createComponent(Dynamic, mergeProps$1({
            get component() {
              return local.as;
            },
            ref(r$) {
              const _ref$ = mergeRefs(el => ref = el, local.ref);
              typeof _ref$ === "function" && _ref$(r$);
            },
            role: "listbox",
