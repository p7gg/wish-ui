import { Orientation } from "@kobalte/utils";
export interface SeparatorRootOptions {
    /** The orientation of the separator. */
    orientation?: Orientation;
}
/**
 * A separator visually or semantically separates content.
 */
export declare const SeparatorRoot: import("@kobalte/utils").PolymorphicComponent<"hr", SeparatorRootOptions>;
