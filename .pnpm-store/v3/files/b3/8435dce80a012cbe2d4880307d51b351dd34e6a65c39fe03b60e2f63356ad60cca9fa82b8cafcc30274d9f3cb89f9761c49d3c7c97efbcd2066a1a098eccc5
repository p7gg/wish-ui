import { OverrideProps } from "@kobalte/utils";
import { ComponentProps, JSX } from "solid-js";
export interface SwitchInputOptions {
    /** The HTML styles attribute (object form only). */
    style?: JSX.CSSProperties;
}
/**
 * The native html input that is visually hidden in the switch.
 */
export declare function SwitchInput(props: OverrideProps<ComponentProps<"input">, SwitchInputOptions>): JSX.Element;
