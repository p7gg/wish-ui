import { JSX } from "solid-js";
/** Call a JSX.EventHandlerUnion with the event. */
export declare function callHandler<T, E extends Event>(event: E & {
    currentTarget: T;
    target: Element;
}, handler: JSX.EventHandlerUnion<T, E> | undefined): boolean;
/** Create a new event handler which calls all given handlers in the order they were chained with the same event. */
export declare function composeEventHandlers<T>(handlers: Array<JSX.EventHandlerUnion<T, any> | undefined>): (event: any) => void;
export declare function isActionKey(): boolean;
export declare function isSelectionKey(): boolean;
export declare function isCtrlKey(e: Pick<KeyboardEvent, "ctrlKey" | "metaKey">): boolean;
