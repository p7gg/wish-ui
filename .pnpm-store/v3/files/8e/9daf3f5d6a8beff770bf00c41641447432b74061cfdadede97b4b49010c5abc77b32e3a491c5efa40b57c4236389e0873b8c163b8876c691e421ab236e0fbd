/**
 * Immutably adds an item at the given index to an array.
 */
export declare function addItemToArray<T extends any[]>(array: T, item: T[number], index?: number): T;
/**
 * Immutably removes an item from an array.
 */
export declare function removeItemFromArray<T>(array: T[], item: T): T[];
