import { OverrideProps } from "@kobalte/utils";
import { ComponentProps } from "solid-js";
export interface TextFieldAreaOptions {
    /** Whether the textarea should adjust its height when the value changes. */
    autoResize?: boolean;
}
/**
 * The native html textarea of the textfield.
 */
export declare function TextFieldTextArea(props: OverrideProps<ComponentProps<"textarea">, TextFieldAreaOptions>): import("solid-js").JSX.Element;
