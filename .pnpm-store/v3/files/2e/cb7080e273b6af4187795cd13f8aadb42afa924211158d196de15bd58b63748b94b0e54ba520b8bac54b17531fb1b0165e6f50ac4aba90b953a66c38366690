import { CreatePressProps, CreatePressResult } from "./types";
export declare const CREATE_PRESS_PROP_NAMES: readonly ["onPressStart", "onPressEnd", "onPressUp", "onPressChange", "onPress", "isDisabled", "preventFocusOnPress", "cancelOnPointerExit", "allowTextSelectionOnPress"];
export declare const PRESS_HANDLERS_PROP_NAMES: readonly ["onKeyDown", "onKeyUp", "onClick", "onPointerDown", "onPointerUp", "onMouseDown", "onDragStart"];
/**
 * Handles press interactions across mouse, touch, keyboard, and screen readers.
 * It normalizes behavior across browsers and platforms, and handles many nuances
 * of dealing with pointer and keyboard events.
 * @param props - Props for the press primitive.
 */
export declare function createPress<T extends HTMLElement>(props: CreatePressProps): CreatePressResult<T>;
