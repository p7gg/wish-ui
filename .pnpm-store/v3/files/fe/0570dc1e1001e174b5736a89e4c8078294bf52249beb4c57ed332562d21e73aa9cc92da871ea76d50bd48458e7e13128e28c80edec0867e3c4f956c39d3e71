import { CreateLongPressProps, CreateLongPressResult } from "./types";
export declare const CREATE_LONG_PRESS_PROP_NAMES: readonly ["onLongPressStart", "onLongPressEnd", "onLongPress", "isDisabled", "threshold"];
/**
 * Handles long press interactions across mouse and touch devices. Supports a customizable time threshold,
 * and normalizes behavior across browsers and devices.
 */
export declare function createLongPress<T extends HTMLElement>(props: CreateLongPressProps): CreateLongPressResult<T>;
